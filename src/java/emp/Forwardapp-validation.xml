<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
       "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
       
<validators>    
    <field name="fname">
        <field-validator type="requiredstring">
            <message>Enter First Name</message>
        </field-validator> 
         <field-validator type="regex">
            <param name="expression">^[A-Za-z ]{1,30}$ </param>
            <message>Name can contain only characters</message>
        </field-validator>
    </field>
    
    <field name="lname">
        <field-validator type="requiredstring">
            <message>Enter Last Name</message>
        </field-validator>   
         <field-validator type="regex">
            <param name="expression">^[A-Za-z ]{1,30}$ </param>
            <message>Name can contain only characters</message>
        </field-validator>
    </field>
   
    <field name="email">
        <field-validator type="email">
          <message>Email Address is not correct</message>
      </field-validator>
        <field-validator type="requiredstring">
            <message>Email is required</message>
        </field-validator>    
    </field>
    
    <field name="mob">
              <field-validator type="regex">
            <param name="expression">^[0-9]{10,10}$</param>
            <message>Phone number format XXXXXXXXXX</message>
        </field-validator>
        <field-validator type="requiredstring">
            <message>Mobile No. is required</message>
        </field-validator>    
    </field>
    
    <field name="members">
        <field-validator type="requiredstring">
            <message>Enter no. of family members</message>
        </field-validator>    
              <field-validator type="regex">
            <param name="expression">^[0-9]{1,4}$</param>
            <message>Enter in Integer form</message>
        </field-validator>
    </field>
    
    
    <field name="harea">
        <field-validator type="requiredstring">
            <message>House area is required</message>
        </field-validator>    
    </field>
    
    <field name="cars">
        <field-validator type="requiredstring">
            <message>No of cars is required</message>
        </field-validator>    
                 <field-validator type="regex">
            <param name="expression">^[0-9]{1,4}$</param>
            <message>Enter in Integer form</message>
        </field-validator>
    </field>
    
    <field name="inc">
        <field-validator type="requiredstring">
            <message>Income is required</message>
        </field-validator>    
        <field-validator type="regex">
            <param name="expression">^[0-9]{1,10}$</param>
            <message>Enter in Integer form</message>
        </field-validator>
    </field>
    
    <field name="fd">
        <field-validator type="requiredstring">
            <message>No.of fixed deposit is required</message>
        </field-validator>    
         <field-validator type="regex">
            <param name="expression">^[0-9]{1,4}$</param>
            <message>Enter in Integer form</message>
        </field-validator>    </field>
    
    <field name="netfd">
        <field-validator type="requiredstring">
            <message> Net FD amount is required</message>
        </field-validator>    
    </field>
    
    <field name="agland">
        <field-validator type="requiredstring">
            <message>Agricultural Land is required</message>
        </field-validator>    
    </field>
    
    <field name="lic">
        <field-validator type="requiredstring">
            <message>Life insurance policy is required</message>
        </field-validator>        
              <field-validator type="regex">
            <param name="expression">^[0-9]{1,4}$</param>
            <message>Enter in Integer form</message>
        </field-validator>
    </field>
    
    <field name="bdflt">
              <field-validator type="requiredstring">
            <message>This field can't b left empty</message>
        </field-validator> 
         <field-validator type="regex">
            <param name="expression">^[0-9]{1,10}$</param>
            <message>Enter in Integer form</message>
        </field-validator>
    </field>
    
    <field name="tax">
        <field-validator type="requiredstring">
            <message>This field can't b left empty</message>
        </field-validator>    
              <field-validator type="regex">
           <param name="expression">^[0-9]{1,10}$</param>
            <message>Enter in Integer form</message>
        </field-validator>
    </field>
</validators>